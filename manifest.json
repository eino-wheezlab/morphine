{
	"name": "Morphine",
	"version": "1.0.5",
	"manifest_version": 2,
	"description": "Regulated distraction.",
	"icons": {
		"16": "images/icons/16.png",
		"48": "images/icons/48.png",
		"128": "images/icons/128.png"
	},
	"browser_action": {
		"default_icon": {
			"19": "images/icons/19-1x.png",
			"38": "images/icons/19-2x.png"
		},
		"default_title": "Morphine",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["scripts/errors.js", "scripts/jsuri.js", "scripts/data.js", "scripts/background.js"]
	},
	"content_security_policy": "script-src 'self' https://d37gvrvc0wt4s1.cloudfront.net https://api.rollbar.com https://ssl.google-analytics.com; object-src 'self'",
	"minimum_chrome_version": "20",
	"offline_enabled": true,
	"options_page": "options.html",
	"permissions": ["tabs"],
	"web_accessible_resources": ["popup.html"]
}